function buildabs()
{
    ABDIR="/var/abs/" 
    abpath=$(find -O3 /var/abs/ -maxdepth 2 -name "$@" -type d | head -1)
    cp -Rv $abpath /dev/shm/
    cd /dev/shm/$1
    #clear
}   

function vpdf(){
