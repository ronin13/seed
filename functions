function download(){
histfile="$HOME/.download_history"

if [[ "$@" =~ .*torrent$ ]];then

    pidof -s rtorrent  &>/dev/null || ~/bin/torrent

    [[ $1 =~ ^http.* ]] && /usr/bin/wget --content-disposition -q $1 > ~/.rtorrent/watch/${1##*/} && echo "$@ ============ $(aria2c -S ~/.rtorrent/watch/${1##*/})===========$(date)" >> $histfile && exit

    cp "$@" ~/.rtorrent/watch/
    # Triggers download complete
    echo "$@ ============ $(aria2c -S $@ | grep Name) ====================$(date)" >> $histfile
    mv "$@" ~/.torrents/

else
    rm ~/.aria2/.temp 2>/dev/null
    #cfile="$HOME/.mozilla/firefox/default.profile/cookies.sqlite"
    #sqlite3 -separator '	'  < $cfile 'select host, "TRUE", path, "FALSE", expiry, name, value from moz_cookies' >|  ~/.aria2/.temp 2>/dev/null
    gid=$(aria2rpc addUri "$@")
    echo "$gid:$@ >===============< $(date)" >> $histfile
    #aria2rpc tellStatus gid
fi
}

function pkgsearch(){
#set -x
if [[ $#  < 1 ]];then
    echo "pkgsearch term pkglist"
    return 1
fi
term="$1"
shift
pkg="$@"
for i in $pkg
do
 for j in `pacman -Ql $i | cut -d " " -f 2`
 do
	grep -l -i $term $j
 done
done
#set +x
}

