function rcstart(){
for d in $@;do
    if [[ -f /var/run/daemons/$d ]];then
        sudo /etc/rc.d/$d stop; sleep 2; sudo /etc/rc.d/$d start
    else
        sudo /etc/rc.d/$d start
    fi
done
}

