function {news,mutt,ncm,vim,weechat,weechat-curses,newsbeuter,ncmpcpp,atop} {
	: ${(AA)muxaliases:=${(z)MUXAL}}
    if [[ -z $TMUX || $1 == -f ]];then
    	[[ $1 == -f ]] && shift
    	if test $muxaliases[$0];then
    		=muxaliases[$0] "$@"
    	else
    		=$0 "$@"
    	fi
    	return
    fi
	app=$0

	if [[ $app == vim ]];then
		vimote tabnew $@
		return
	fi

	: ${(AA)appMap:=${(z)WSETS}}
	winum=$(~/bin/tux tnum $app)

	if [[ $? != 0 ]];then
		print "Failed"
		return
	fi

    tmux join-pane -s $appMap[$app]:$winum -p 80 
    if [[ $? -ne 0 ]];then
    	 $0 -f "$@"
	fi

#    if [[ -n $1 ]];then
#		$muxaliases[$app] $@ 
#	fi
}

function {reboot,sdown} {
    typeset -A map
    map=(sdown '~/bin/down' reboot 'command reboot')
    command=$0
    echo "Sure y/N"
    read -q && eval "$map[$command]"
}

